{
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "plugins": ["@typescript-eslint", "prettier"],
  "rules": {
    "no-irregular-whitespace": ["error", {"skipStrings": true, "skipComments": true}]
  },
  "parser": "@typescript-eslint/parser"
}